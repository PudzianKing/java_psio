package tablice;

public class zad1 {
	public static void main(String[] args) {
		int[][]tab=new int[2][3];
		int l=0;
		for(int i=0;i<tab.length;i++){
			for(int j=0;j<tab[i].length;j++){
				tab[i][j]=l++;
			}
		}
		for(int i=0;i<tab.length;i++){
			for(int j=0;j<tab[i].length;j++){
				System.out.print(tab[i][j]+" ");
			}
			System.out.println();
		}
	}
}
